public class NumberPalindrome {
    public static boolean isPalindrome(int number){
        int sum=0;
        int or=number;
        while (number!=0){
            int digit=number%10;
            sum=(sum*10)+digit;
            number=number/10;
        }
        System.out.println(sum);
        if (or==sum)
        {
            return true;
        }
        else {return false;}

    }
}
